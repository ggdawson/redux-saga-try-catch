{
  "name": "redux-saga-catch",
  "version": "0.0.1",
  "author": "Garrett Dawson",
  "description": "A saga utility to reduce flow control boilerplate",
  "homepage": "https://github.com/killtheliterate/redux-saga-catch",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "bugs": {
    "url": "https://github.com/killtheliterate/redux-saga-catch/issues"
  },
  "peerdependencies": {
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "@types/redux-saga": "^0.10.5",
    "jest": "^23.1.0",
    "rollup-plugin-typescript2": "^0.14.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.9.1"
  },
  "jest": {
    "transform": {
      "\\.(ts)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts}"
    ],
    "coveragePathIgnorePatterns": []
  },
  "keywords": [
    "redux-saga",
    "redux"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/killtheliterate/redux-saga-catch.git"
  },
  "scripts": {
    "build": "rm -rf dist && npx rollup -c",
    "prepublish": "npm run build",
    "test": "jest"
  }
}
